{
    "filters": {
        "include_paths": [
            "./lib/filters"
        ],
        "rules": [
            {
                "applies": ["/*"],
                "ignores": ["login", "auth/facebook/*", "information/*", "room/*", "redirect", "/\\.[a-zA-z]*/"],
                "name_space": "authentication_filter.index",
                "as": "authentication"
            },
            {
                "applies": ["/*"],
                "ignores": ["login", "logout", "auth/facebook/*", "information/*", "/\\.[a-zA-z]*/"],
                "name_space": "lastLoginDateTimeFilter.index",
                "as": "lastLoginDateTime"
            }
        ]
    }
}
